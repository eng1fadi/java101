import java.util.Scanner;
public class aaaa {
    public static void main(String[] args) {
        Scanner inp = new Scanner(System.in);
        int a=0,b=1,c,s;
        System.out.print("Febunachi elemanin sayisi giriniz: ");
        s=inp.nextInt();
        for(int i=0;i<=s;i++){
            c=a+b;
            System.out.println(a+"+"+b+"="+c);
            a=b;
            b=c;
        }
    }
}
