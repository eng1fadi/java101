import java.util.Scanner;
public class aaaa {
    public static void main(String[] args) {
        Scanner inp = new Scanner(System.in);
        int s,k=0;
        System.out.print("Sayi griniz: ");
        s=inp.nextInt();
        for(int i=1;i<s;i++){
            if(s%i==0){
                k=k+i;
            }
        }
        if(s==k){
            System.out.println(s+" Mukemmel sayidir.");
        }else{
            System.out.println(s+" Mukemmel sayi degildir.");
        }
    }
}
