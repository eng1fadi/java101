import java.util.Scanner;
public class aaaa {
    static int us(int a, int b){
        if (b<=0)
            return 1;
        else {
            b--;
            return a*us(a, b);
        }
    }
    public static void main(String[] args) {
        Scanner inp = new Scanner(System.in);
        int m,n;
        System.out.print("Taban değeri giriniz : ");
        m= inp.nextInt();
        System.out.print("Taban değeri giriniz : ");
        n= inp.nextInt();
        System.out.print("Sonuc= "+us(m,n));
    }
}
