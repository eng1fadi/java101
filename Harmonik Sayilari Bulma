import java.util.Scanner;
public class aaaa {
    public static void main(String[] args) {
        Scanner inp = new Scanner(System.in);
        System.out.print("N sayisini giriniz: ");
        int n=inp.nextInt();
        double s=0.0;
        for (int i=1;i<=n;i++){
            s+=(1.0/i);
        }
        System.out.print(s);
    }
}
