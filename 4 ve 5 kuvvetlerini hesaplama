import java.util.Scanner;
public class aaaa {
    public static void main(String[] args) {
        Scanner inp = new Scanner(System.in);
        int y;
        System.out.print("Sayi giriniz: ");
        y=inp.nextInt();
        for (int i=1;i<=y;i++) {
            System.out.println(i + "^3= " + i * i * i);
            System.out.println(i + "^4= " + i * i * i * i);
        }
    }
}
