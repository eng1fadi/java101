import java.util.Scanner;
public class aaaa {
    public static void main(String[] args) {
        Scanner inp = new Scanner(System.in);
        int y,sum=0,m=0;
        System.out.print("Sayi giriniz: ");
        y=inp.nextInt();
        for (int i=0;i<=y;i++)
            if (i%3==0||i%4==0){
                sum=sum+i;
                m++;
            }
        System.out.print("3 ve 4'e tam bölünen sayıların ortalaması="+(sum/m));
    }
}
