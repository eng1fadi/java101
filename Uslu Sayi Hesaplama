import java.util.Scanner;
public class aaaa {
    public static void main(String[] args) {
        Scanner inp = new Scanner(System.in);
        int n,r,sn=1;
        System.out.print("Ussu alinacak sayi: ");
        n=inp.nextInt();
        System.out.print("Us olacak sayi: ");
        r=inp.nextInt();
        for (int i=1;i<=r;i++)
            sn=sn*n;
        System.out.println(n+"^"+r+"= "+sn);
    }
}
